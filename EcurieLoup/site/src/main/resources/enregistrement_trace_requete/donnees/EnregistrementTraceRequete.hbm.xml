<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="enregistrement_trace_requete.donnees.EnregistrementTraceRequete" table="EnregistrementTraceRequete">

		<id name="id" column="id">
			<generator class="increment"></generator>
		</id>

		<property name="date" column="date" />
		<property name="userAgent" column="userAgent" />
		<property name="referer" column="referer" />
		
		<property name="uri" column="uri" />
		<property name="remoteAdress" column="remoteAdress" />
		
		
		<set name="parametresRequeste" table="ParametreEnregistrementTraceRequete" lazy="false">
			<key column="requete" />
			<one-to-many class="enregistrement_trace_requete.donnees.ParametreEnregistrementTraceRequete" />
		</set>	
	</class>

</hibernate-mapping>